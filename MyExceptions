package Lesson2;

public class MyExceptions {
    public static void main(String[] args) {
       int [] arr = {1,3,4,54};
       try {arr[5] = 7;}
       catch (ArrayIndexOutOfBoundsException e) {
           System.out.println(e.getMessage());
       }
         try {
             for (int i=0; i<=arr.length; i++) {
               System.out.println(arr[i]);
             }
            }
           catch(ArrayIndexOutOfBoundsException e) {
               System.out.println(e.getMessage());
            }
    }
}
